{
  "regexManagers": [
    {
      "fileMatch": ["^\\.github/workflows/.*\\.ya?ml$"],
      "matchStrings": [
        "uses:\\s*['\"]?(?<depName>[\\w.-]+/StarsIndustries/.github/workflows/renotest.yaml)@(?<currentValue>[\\w.-]+)['\"]?"
      ],
      "depNameTemplate": "{{{depName}}}",
      "datasourceTemplate": "github-branches",
      "versioningTemplate": "loose",
      "replaceStringTemplate": "testbranch"
    }
  ]
}
