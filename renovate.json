{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": [".github/workflows/.*\\.yml$"],
      "matchStrings": [
        "uses: (admsallai/StarsIndustries)@([\\w\\-]+)"
      ],
      "datasourceTemplate": "github-branches",
      "depNameTemplate": "{{{depName}}}",
      "versioning": "loose",
      "extractVersion": "^(?<version>.*)$",
      "allowedVersions": "/.*/",
      "depNameTemplate": "{{{depName}}}"
    }
  ]
}
